<configuration>
  <configSections>
    <section name="castle" type="Castle.Windsor.Configuration.AppDomain.CastleSectionHandler, Castle.Windsor"/>
  </configSections>
  <castle>
    <include uri="file://Config/facilities.config"/>
    <include uri="file://Config/services.config"/>
  </castle>
  <connectionStrings>
    <add name="ApplicationServices" connectionString="Server=PC-YANG\SQLSERVER;Initial Catalog=MISDB;User ID=sa;Password=123456;" providerName="System.Data.SqlClient"/>
